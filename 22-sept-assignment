4.
#include<stdio.h>
int rev(int n);
int main()
{
int n,c;
printf("enten n");
scanf("%d",&n);
c=rev;
printf("%d",c);
return 0;
}
int rev(int n)
{
int temp;
temp=0;
wnhile(n!=0)
{
r=n%10;
n=n/10;
temp=temp*10+r;
}
return temp;
}
