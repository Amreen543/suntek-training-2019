9.
#include<stdio.h>
int mul(int n);
{
int i,c;
for(i=1;i<10;i++)
{
c=n*i;
printf("\n%d*%%d",n,i,c);
}
}
int main()
{
int n;
printf("enter n value");
scanf("%d",&n);
mul(n);
return 0;
}
