1.#include<stdio.h>
int cou(int* a,int count,int n);
int main()
{
int a[50],n,i,count;
scanf("%d",&n);
for(i=0;i<n;i++)
{
scanf("%d",&a[i]);
}
scanf("%d",&count);
printf("%d",cou(a,count,n));
return 0;
}
int cou(int* a,int count,int n)
{
int i,temp=0;
for(i=0;i<n;i++)
{
if(count==a[i])
temp++;
}
if(temp==0)
return -1;
return temp;
}
