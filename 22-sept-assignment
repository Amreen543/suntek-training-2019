2.
#include<stdio.h>
int cou(int* a,int n);
int main()
{
int a[100];
int n,i;
scanf("%d",&n);
for(i=0;i<n;i++)
{
scanf("%d",&a[i]);
}
count(a,n);
return 0;
}
int cou(int* a,int n)
{
int i,i,k,flag,temp;
for(i=0;i<n;i++)
{
flag=0;
for(j=i+1;j<n;j++)
{
if(a[i]==a[j])
{
flag++;
a[j]=temp;
}
}
if(a[i]!=temp)
{

printf("%d is repeated %d times\n",a[i],flag+1);
}
}
}
